  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.7.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "consola": "^3.4.2",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "gradient-string": "^3.0.0",
    "handlebars": "^4.7.8",
    "jsonc-parser": "^3.3.1",
    "minimist": "^1.2.8",
    "picocolors": "^1.1.1",
    "tinyglobby": "^0.2.15",
    "zod": "^4.1.12"
