{
  "name": "initex",
  "version": "1.3.13",
  "bin": {
    "initex": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "tsx src/index.ts --mode test",
    "test:dist": "tsx bin/cli.js --mode test",
    "prepare": "npm run build",
    "lint-format": "biome check --write"
