{
	"name": "MySQL + Sequelize + NodeCache + Socket",
	"predefined": true,
	"preset": {
		"name": ".",
		"expressVersion": "latest",
		"db": {
			"enable": true,
			"provider": "mysql",
			"connectionString": "mysql://root:password@localhost:3306",
			"orm": "sequelize",
			"name": "my_database"
		},
		"cache": {
			"enable": true,
			"service": "nodecache"
		},
		"auth": {
			"enable": false
		},
		"git": true,
		"socket": true,
		"language": "ts"
	}
}
